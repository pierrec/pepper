module hashmap_test @test;
import hashmap;

fn void hashmap()
{
    String[] tcases = {
        "hello", "world", "!",
    };
    HashMap hm;
    foreach (i, tc : tcases)
    {
        assert(i == hm.len);
        assert(!hm.has(tc), "'%s' found", tc);
        hm.add(tc);
        assert(i == hm.len - 1);
        assert(hm.has(tc), "'%s' not found", tc);
    }
}